<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>个人主页</title>
</head>
<body>
<h4>个人信息</h4>
<a>查看/修改</a>
<form action="">
    <label>
        <p>管理员工号</p>
        <input type="text"/>
    </label>
    <label>
        <p>管理员类型</p>
        <select name="" id="">
            <option>请选择</option>
            <option>系统管理员</option>
            <option>物业管理员</option>
        </select>
    </label>
    <label>
        <p>管理员性别</p>
        <select name="" id="">
            <option>请选择</option>
            <option>男</option>
            <option>女</option>
        </select>
    </label>
    <label>
        <p>管理员电话号码</p>
        <input type="text"/>
    </label>
    <label>
        <p>管理员职称</p>
        <input type="text"/>
    </label>
    <label>
        <p>管理员身份证号码</p>
        <input type="text"/>
    </label>
    <input type="submit" value="提交">
</form>

</body>
</html>
